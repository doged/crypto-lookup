{
  "manifest_version": 2,

  "name": "Crypto Lookup",
  "description": "This Google Chrome extension allows you to view bitcoin, litecoin, and dogecoindark addresses, transactions (txid), and blocks (block hashes).",
  "version": "0.2.2",

  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png",
    "128": "images/logo-128.png"
  },

  "permissions": [
    "storage",
    "contextMenus"
  ],

  "background": {
    "scripts": ["libs/underscore-min.js","libs/contextManagement.js","background.js"],
    "persistent": false
  },

  "options_page": "options.html"

}
